{
 "version": 3,
 "installCommand": "npm install --legacy-peer-deps",
 "builds": [
 {
 "src": "api/index.js",
 "use": "@vercel/node"
 },
 {
 "src": "package.json",
 "use": "@vercel/static-build",
 "config": { "distDir": "build" }
 }
 ],
 "routes": [
 {
 "src": "api/(.*)",
 "dest": "api/index.js"
 }
 ]
}
