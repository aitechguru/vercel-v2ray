{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "next build",
  "cleanUrls": true,
  "outputDirectory": "public",  
  "builds": [
    { "src": "/api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/api/index.js" },    
    { "src": "/api/(.*)", "dest": "/api/$1"}
  ]
}
